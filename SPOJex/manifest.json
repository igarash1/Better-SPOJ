{
  "name": "SPOJex",
  "version": "0.1",
  "description": "An chrome extension for SPOJ users.",
  "background": {
    "page": "background.html"
  },
  "manifest_version": 2,
  "author": "Koki Igarashi",
  "browser_action": {
    "name": "Manipulate HTML",
    "default_icon": "icon48.png",
    "default_title": "SPOJex"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.spoj.com/*",
        "https://www.spoj.com/*"
      ],
      "js": [
        "jquery-3.5.1.min.js",
        "contentScript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
